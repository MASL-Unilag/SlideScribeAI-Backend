{
  "name": "slide-scribe-ai",
  "version": "1.0.0",
  "description": "note-to-slide generation",
  "main": "dist/main",
  "scripts": {
    "build:prod": "tsc -p tsconfig.json",
    "build:dev": "tsc -p tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "start:prod": "npm run clean && rimraf tsconfig.tsbuildinfo && npm run build:prod && node dist/main.js",
    "start:dev": "npm run clean && rimraf tsconfig.tsbuildinfo && npm run build:dev && NODE_ENV=development && nodemon main"
  },
  "keywords": [
    "notes",
    "slides",
    "AI"
  ],
  "author": "github.com/tdadadavid",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^29.5.8",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.3.1",
    "eventemitter2": "^6.4.9",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.9.0",
    "form-data": "^4.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "mailgun.js": "^9.3.0",
    "moment": "^2.29.4",
